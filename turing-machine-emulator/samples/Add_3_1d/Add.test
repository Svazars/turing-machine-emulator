// tests addition of 3 to inversed binary number, result is inversed too

test_empty : #                -> #
test_zero  : #0#              -> #11#
test_1     : #1#              -> #001#
test_2     : #01#             -> #101#
test_3     : #11#             -> #011#
test_4     : #001#            -> #111#
test_5     : #101#            -> #0001#
test_6     : #011#            -> #1001#
test_7     : #111#            -> #0101#
test_127   : #1111111#        -> #01000001#
test_567   : #1110110001#     -> #0101110001#

