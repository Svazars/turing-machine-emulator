// tests addition of 1 to inversed binary number, result is inversed too

test_empty : #                -> #
test_zero  : #0#              -> #1#
test_1     : #1#              -> #01#
test_2     : #01#             -> #11#
test_3     : #11#             -> #001#
test_4     : #001#            -> #101#
test_5     : #101#            -> #011#
test_6     : #011#            -> #111#
test_7     : #111#            -> #0001#
test_127   : #1111111#        -> #00000001#
test_567   : #1110110001#     -> #0001110001#
test_1023  : #1111111111#     -> #00000000001#